{
  "name": "FESK",
  "version": "1.0.0",
  "description": "Front End Starter Kit",
  "author": "Dragan Eror",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "dependencies": {
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-foreach": "^0.1.0",
    "gulp-gzip": "^1.4.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-inject": "^4.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob": "^1.0.8",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp-transform": "^3.0.5",
    "gulp-uglify": "^3.0.0",
    "gulp.spritesmith": "^6.5.1",
    "merge-stream": "^1.0.1",
    "node-notifier": "^5.1.2",
    "node-sass-import-once": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7",
    "postcss-clean": "^1.0.3",
    "postcss-svg": "^2.1.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "vinyl-buffer": "^1.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:svg build:css build:font build:js pattern-lab:css pattern-lab:twig build:notify",
    "build:css": "gulp build:css:development",
    "build:js": "gulp build:js:development",
    "build:svg": "gulp svg:sprite",
    "build:font": "gulp font:webfontloader:recreate",
    "build:notify": "gulp notifications:build",
    "build:pattern-lab": "npm-run-all pattern-lab:css pattern-lab:twig",
    "build:production": "npm-run-all build:production:css build:production:js cleanup:tmp build:notify",
    "build:production:css": "gulp build:css:production",
    "build:production:js": "gulp build:js:production",
    "pattern-lab:css": "gulp pattern-lab:sass",
    "pattern-lab:twig": "php pattern-lab/core/console --generate",
    "server:pattern-lab": "php pattern-lab/core/console --server",
    "cleanup": "npm-run-all cleanup:*",
    "cleanup:assets": "gulp build:del:assets",
    "cleanup:templates": "gulp build:del:templates",
    "cleanup:tmp": "gulp build:del:tmp",
    "watch": "gulp watch"
  }
}
